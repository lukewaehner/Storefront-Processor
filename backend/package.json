{
  "name": "storefront-processor-backend",
  "version": "0.0.1",
  "description": "Backend for multi-tenant e-commerce platform",
  "type": "module",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:ts-transpile": "TS_NODE_TRANSPILE_ONLY=true nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "NODE_ENV=test node --experimental-vm-modules node_modules/.bin/jest --config jest-unit.config.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:test:setup": "NODE_ENV=test prisma migrate reset --force --skip-seed",
    "prisma:migrate:test:deploy": "NODE_ENV=test prisma migrate deploy",
    "prisma:seed:test": "NODE_ENV=test ts-node prisma/seed-test.ts",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@prisma/client": "^6.7.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/mapped-types": "^2.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/bcrypt": "^5.0.1",
    "@types/cookie-parser": "^1.4.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.17.46",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "prisma": "^6.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  }
}
